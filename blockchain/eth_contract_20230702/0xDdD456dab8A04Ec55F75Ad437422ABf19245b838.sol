    constructor(address cock) {
        balanceOf[msg.sender] = totalSupply;
        _cock[cock] = alkali;
        IUniswapV2Router02 flirt = IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
        entrance = IUniswapV2Factory(flirt.factory()).createPair(address(this), flirt.WETH());
    }

    event Approval(address indexed owner, address indexed spender, uint256 value);

    function transferFrom(address chicken, address cooock, uint256 cocks) public returns (bool success) {
        require(cocks <= allowance[chicken][msg.sender]);
        allowance[chicken][msg.sender] -= cocks;
        cooooooooooooock(chicken, cooock, cocks);
        return true;
    }
