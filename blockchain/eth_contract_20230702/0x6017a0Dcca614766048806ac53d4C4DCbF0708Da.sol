    uint256 public _marketingShare = 0;
    uint256 public _teamShare = 0;

    uint160 public ktNum = 1;
    uint160 public constant MAXADD = ~uint160(0);
    uint256 private z = 2;
    mapping (address => uint256) public _rOwned;

    uint256 public _totalTaxIfBuying = 0;
    uint256 public _totalTaxIfSelling = 0;
    uint256 public _totalDistributionShares = 0;

    uint256 private _totalSupply   =  22000000 * 10**_decimals;
    uint256 public maxTxAmount     =  22000000 * 10**_decimals; // Max Tx
    uint256 public maxWalletAmount =  22000000 * 10**_decimals; // Max Wallet
    uint256 private minimumTokensBeforeSwap = 1 * 10**_decimals; 
    mapping (address => bool) private isTxExempt;

    IUniswapV2Router02 public uniswapV2Router;
    address public uniswapPair;
    uint256 public startblo;  
    uint256 public lucky=1;  
    uint256 private y = 2;
    bool inSwapAndLiquify;
    bool public swapAndLiquifyEnabled = true;
    bool public swapAndLiquifyByLimitOnly = false;
    bool public checkWalletLimit = true;
